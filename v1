import java.util.ArrayList;

public class Main {
    public static void main(String[] args) {
        // Создаем динамический список из 4 элементов
        ArrayList<String> list = new ArrayList<>();
        list.add("1");
        list.add("2");
        list.add("3");
        list.add("4");

        // Выводим на экран элемент 3
        System.out.println(list.get(2));
    }
}
